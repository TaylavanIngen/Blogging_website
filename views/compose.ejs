<%- include("partials/header")-%>

<h1>Compose a blogpost </h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
  <label for="titleField">Title</label>
  <input class="form-control" type="text" name="titleField">
  <label for="inputField">Post</label>
  <textarea class="form-control" name="inputField" rows="8" cols="80"></textarea>
  </div>
  <button class= "btn btn-primary" type="submit" name="button">Submit</button>
</form>

<%- include("partials/footer")-%>
